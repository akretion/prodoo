'use strict';

angular.module('prodapps').provider('prodooConfig', [ function prodooConfigProvider() {
    this.config = {
        odooServer: '{{ odoo_path }}',
        db: 'sd_production',
        refreshInterval: 5000,
        fetchLimit: 50,
        pyWebDriver: '{{ pyWebDriver_path }}',
        displayLimit: 50
    };

    this.$get = [function () {
            return this.config;
    }];

    return this;
}])